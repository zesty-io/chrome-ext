{
  "name": "Zesty.io WebEngine View Explorer",
  "version": "1.0",
  "description": "A tool for Zesty.io End Users to gain insight into their preview environment views.",
  "permissions": [
    "declarativeContent",
    "storage",
    "cookies",
    "*://*.zesty.io/",
    "*://*.zesty.io/*",
    "webRequest",
    "webRequestBlocking",
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "page_action": {
    "default_icon": {
      "16": "images/chrome-ext-icons-16.png",
      "32": "images/chrome-ext-icons-32.png",
      "48": "images/chrome-ext-icons-48.png",
      "128": "images/chrome-ext-icons-128.png"
    }
  },
  "icons": {
    "16": "images/chrome-ext-icons-16.png",
    "32": "images/chrome-ext-icons-32.png",
    "48": "images/chrome-ext-icons-48.png",
    "128": "images/chrome-ext-icons-128.png"
  },
  "manifest_version": 2
}
